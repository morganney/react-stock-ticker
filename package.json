{
  "name": "react-stock-ticker",
  "version": "1.0.0-rc.0",
  "description": "React stock ticker component for animating price changes.",
  "type": "module",
  "main": "dist/stockTicker.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/stockTicker.d.ts",
        "default": "./dist/stockTicker.js"
      },
      "require": {
        "types": "./dist/cjs/stockTicker.d.cts",
        "default": "./dist/cjs/stockTicker.cjs"
      }
    }
  },
  "engines": {
    "node": ">=22.6.0"
  },
  "scripts": {
    "prettier": "prettier --write .",
    "checktypes": "tsc --noEmit",
    "lint": "eslint",
    "test": "vitest run --coverage",
    "build": "duel",
    "build:vite": "npm run build && vite build",
    "build:deploy": "npm run build && vite build --base=/morganney/",
    "preview": "npm run build:vite && vite preview",
    "prepack": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "stock",
    "ticker",
    "animate",
    "price"
  ],
  "author": "Morgan Ney <morganney@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@knighted/duel": "^2.0.0-rc.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.13.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "prettier": {
    "semi": false,
    "bracketSameLine": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "overrides": {
    "vite": "^6.0.2"
  }
}
